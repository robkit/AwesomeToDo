<h1>Sign Up</h1>
 
    <%= form_tag(users_url, method: 'post') do %>

      <p><%= label_tag :username %><%= text_field_tag :name %></p>
      <p><%= label_tag :nickname %><%= text_field_tag :nickname %></p>
      <p><%= label_tag :email %><%= text_field_tag :email %></p>
      <p><%= label_tag :password %><%= password_field_tag :password %></p>
      
      <%= submit_tag "Sign me up!" %>

    <% end %>
  



